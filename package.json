  ],
  "scripts": {
    "docs:dev": "styleguidist server",
    "docs:build": "styleguidist build --docs=true",
    "build:clean": "rimraf es6 lib dist",
    "prebuild": "npm run typecheck && npm run build:clean",
    "build": "npm run build:es6 && npm run build:cjs && npm run build:umd",
    "build:es6": "tsc --module es2015 --target es2015 --outDir es6",
    "build:cjs": "tsc --module commonjs --target es5 --outDir lib",
    "build:umd": "cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "start": "http-server ./styleguide -g --cors",
    "lint": "eslint --ignore-path .gitignore",
    "lint:fix": "eslint --ignore-path .gitignore --fix",
    "test": "cross-env NODE_ENV=test jest --runInBand",
    "test:watch": "npm run test -- --watch",
    "test:cov": "npm run test -- --coverage",
    "prettify": "prettier \"+(src)/**/*.+(js|jsx|ts|tsx)\" --write",
    "typecheck": "tsc --noEmit",
    "dev": "yarn run docs:dev"
  },
  "lint-staged": {
    "*.{js|jsx|ts|tsx}": [
      "npm run lint:fix",
      "git add --force"
    ],
    "*.{md,json}": [
      "prettier --write",
      "git add --force"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
